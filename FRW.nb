  3.964562026246537*^9}},ExpressionUUID->"349d26a9-7e3d-4df2-98d3-\
5417b3926723"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Zerodd", "=", 
   RowBox[{"Rdd", "-", 
    RowBox[{
     FractionBox["1", "2"], "R", " ", "gdd"}], " ", "+", " ", 
    RowBox[{"\[CapitalLambda]", " ", "gdd"}], "-", 
    RowBox[{"\[Kappa]", " ", "Tdd"}]}]}], ";"}]], "Input",

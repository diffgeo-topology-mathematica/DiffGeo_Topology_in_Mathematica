Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"r", "=", 
   RowBox[{"Sqrt", "[", 
    RowBox[{
     RowBox[{"x", "^", "2"}], "+", 
     RowBox[{"y", "^", "2"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Theta]", "=", 
   RowBox[{"ArcTan", "[", 
    RowBox[{"y", "/", "x"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ExteriorProduct", "[", 
   RowBox[{
    RowBox[{"d", "[", "r", "]"}], ",", 
    RowBox[{"d", "[", "\[Theta]", "]"}]}], "]"}], "//", 
  "FullSimplify"}]}], "Input",
 CellChangeTimes->{{3.894625769955473*^9, 3.894625816633029*^9}},
 CellLabel->"In[72]:=",ExpressionUUID->"96b9f96f-9456-4405-bcc0-e8e52af64d98"],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"(\"\>", "\[InvisibleSpace]", 
   FractionBox["1", 
    SqrtBox[
     RowBox[{
      SuperscriptBox["x", "2"], "+", 
      SuperscriptBox["y", "2"]}]]], "\[InvisibleSpace]", "\<\") \"\>", 
   "\[InvisibleSpace]", "\<\"d\"\>", "\[InvisibleSpace]", "x", 
   "\[InvisibleSpace]", "\<\"\"\>", "\[InvisibleSpace]", "\<\" ^ \"\>", 
   "\[InvisibleSpace]", "\<\"d\"\>", "\[InvisibleSpace]", "y", 
   "\[InvisibleSpace]", "\<\"\"\>"}],
  SequenceForm[
  "(", ($CellContext`x^2 + $CellContext`y^2)^Rational[-1, 2], ") ", 
   "d", $CellContext`x, "", " ^ ", "d", $CellContext`y, ""],
  Editable->False]], "Output",
 CellChangeTimes->{{3.89462580242982*^9, 3.8946258169945664`*^9}},
 CellLabel->"Out[74]=",ExpressionUUID->"960c63ce-eba3-4c3f-912e-3490e080a0f0"]
}, Open  ]],

